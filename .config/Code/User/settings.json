{
    "go.useLanguageServer": true,
    "workbench.colorTheme": "Office Theme (Publisher)",
    "workbench.statusBar.visible": true,
    "workbench.settings.editor": "ui",
    "workbench.iconTheme": "vscode-simpler-icons",
    "window.zoomLevel": 1,
    "diffEditor.ignoreTrimWhitespace": false,
    "jest.autoRun": "watch",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs":"active",
    "launch": {
      "configurations": [
        {
          "name": "Launch Koa-Server",
          "type": "node",
          "request": "launch",
          "program": "${workspaceRoot}/sources/server.js",
          "stopOnEntry": false,
          "args": [],
          "cwd": "${workspaceRoot}",
          "preLaunchTask": null,
          "runtimeExecutable": null,
          "runtimeArgs": [
          ],
          "console": "internalConsole",
          "outputCapture": "std",
          "sourceMaps": false
        },
        {
          "name": "Debug migration",
          "type": "node",
          "request": "launch",
          "program": "${workspaceRoot}/migrations/VERBU-9653/migrate.js",
          "stopOnEntry": false,
          "args": [],
          "cwd": "${workspaceRoot}",
          "preLaunchTask": null,
          "runtimeExecutable": null,
          "runtimeArgs": [
          ],
          "env": {
            "USE_SSH": "true"
          },
          "console": "internalConsole",
          "outputCapture": "std",
          "sourceMaps": false
        },
        {
          "name": "Attach to Process",
          "type": "node",
          "request": "attach",
          "processId": "${command:PickProcess}",
          "port": 5858,
          "sourceMaps": false
        },
        {
          "name": "RemoteDebug",
          "type": "node",
          "request": "attach",
          "port": 9229,
          "remoteRoot": "/app",
          "sourceMaps": false
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Jest Debug Tests",
          "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
          "cwd": "${workspaceFolder}",
          "args": [
            "--config=${workspaceFolder}/jest.config.json",
            "--runInBand",
            "--no-cache"
          ],
          "console": "internalConsole",
          "outputCapture": "std",
          "internalConsoleOptions": "neverOpen"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Jest Debug Current File",
          "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
          "cwd": "${workspaceFolder}",
          "args": [
            "--config=${workspaceFolder}/jest.config.json",
            "--runInBand",
            "--no-cache",
            "--testPathPattern=${fileBasenameNoExtension}"
          ],
          "console": "internalConsole",
          "outputCapture": "std",
          "internalConsoleOptions": "neverOpen"
        },
        {
          "type": "go",
          "name": "Launch Go",
          "request": "launch",
          "mode": "auto",
          "program": "${fileDirname}",
          "env": {},
          "args": []
        }
      ],
      "compounds": []
    }
}
