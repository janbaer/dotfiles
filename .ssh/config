  Port 26
  Identityfile ~/.ssh/id_ed25519
  AddressFamily inet

Host bitbucket.check24.de
  Identityfile ~/.ssh/id_ed25519
  IdentitiesOnly yes

Host *.intern.bu.check24.de
  Port 44022
  Identityfile ~/.ssh/id_ed25519-sk
