  ControlMaster auto
  ControlPath ~/.ssh/socket-%r@%h:%p
  ControlPersist 10m

Host *
  # StrictHostKeyChecking yes
  Identityfile ~/.ssh/id_ed25519
  ServerAliveInterval 60
